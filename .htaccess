
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /ScoppechioRepo/Scoppechio/styleguide/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /ScoppechioRepo/Scoppechio/styleguide/index.php [L]
</IfModule>

# END WordPress